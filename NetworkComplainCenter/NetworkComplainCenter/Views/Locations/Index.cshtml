                    </tr>
                    <tr>
                        <td data-th="Id"><span class="bt-content">2</span></td>
                        <td data-th="Department"><span class="bt-content">Lab</span></td>
                        <td data-th="RoomNo"><span class="bt-content">404</span></td>
                        <td data-th="Floor"><span class="bt-content">4</span></td>
                        <td><span class="glyphicon glyphicon-remove"></span> &nbsp;<span class="glyphicon glyphicon-edit"></span></td>

                    </tr>
                    <tr>
                        <td data-th="Id"><span class="bt-content">3</span></td>
                        <td data-th="Department"><span class="bt-content">SRO</span></td>
                        <td data-th="RoomNo"><span class="bt-content">1</span></td>
                        <td data-th="Floor"><span class="bt-content">2</span></td>
                        <td><span class="glyphicon glyphicon-remove"></span> &nbsp;<span class="glyphicon glyphicon-edit"></span></td>

                    </tr>
                    <tr>
                        <td data-th="Id"><span class="bt-content">4</span></td>
                        <td data-th="Department"><span class="bt-content">Accounts</span></td>
                        <td data-th="RoomNo"><span class="bt-content">1</span></td>
                        <td data-th="Floor"><span class="bt-content">2</span></td>
                        <td><span class="glyphicon glyphicon-remove"></span> &nbsp;<span class="glyphicon glyphicon-edit"></span></td>

                    </tr>
                    <tr>
                        <td data-th="Id"><span class="bt-content">5</span></td>
                        <td data-th="Department"><span class="bt-content">Labs</span></td>
                        <td data-th="RoomNo"><span class="bt-content">603</span></td>
